#include <stdio.h>
#include <string.h>
void bitstuff(char data[])
{
    char stuffed[100];
    int i,j=0,count=0;
    for(i=0;i<strlen(data);i++)
    {
        stuffed[j++]=data[i];
        if(data[i]=='1')
        {
            count++;
            if(count==5)
            {
                stuffed[j++]='0';
                count=0;
            }
        }
        else
        {
            count=0;
        }
    }
    stuffed[j]='\0';
    printf("Stuffed frame: %s\n",stuffed);
}
int main() 
{
    char data[200];
    printf("Enter input data: ");
    scanf("%s",data);
    bitstuff(data);
    return 0;
}
